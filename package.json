{
     "name": "gymscore-backend",
     "version": "1.0.1",
     "description": "Backend for GymScore application",
     "main": "src/index.ts",
     "engines": {
          "node": "22.x"
     },
     "scripts": {
          "dev": "ts-node-dev --respawn src/index.ts",
          "build": "tsc",
          "start": "node dist/index.js",
          "prod": "NODE_ENV=production node dist/index.js",
          "lint": "eslint . --ext .ts",
          "lint:fix": "eslint . --ext .ts --fix",
          "format": "prettier --write \"src/**/*.{ts,js,json}\"",
          "test": "jest",
          "test:watch": "jest --watch",
          "predeploy": "npm run build",
          "deploy": "npm run predeploy && npm run start",
          "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --no-audit --no-fund && npm run build"
     },
     "dependencies": {
          "bcryptjs": "^2.4.3",
          "cors": "^2.8.5",
          "dotenv": "^16.0.3",
          "express": "^4.18.2",
          "express-rate-limit": "^8.2.1",
          "jsonwebtoken": "^9.0.0",
          "mongoose": "^7.2.0",
          "socket.io": "^4.8.1",
          "xlsx": "^0.18.5",
          "zod": "^3.25.76"
     },
     "devDependencies": {
          "@types/express": "^4.17.17",
          "@types/jest": "^30.0.0",
          "@types/jsonwebtoken": "^9.0.1",
          "@types/mongoose": "^5.11.97",
          "@types/node": "18.19.19",
          "jest": "^30.2.0",
          "list": "^2.0.19",
          "ts-jest": "^29.4.6",
          "ts-node": "^10.9.2",
          "ts-node-dev": "^2.0.0",
          "typescript": "5.4"
     },
     "keywords": [],
     "author": "GymScore Team",
     "license": "MIT"
}
